  "author": "Wang Zuo",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "babel-preset-swiftcarrot": "^1.0.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@emotion/core": "^10.0.14",
    "@swiftcarrot/color-fns": "^3.0.4",
