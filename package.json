  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftcarrot/react-input-color.git"
  },
  "bugs": {
    "url": "https://github.com/swiftcarrot/react-input-color/issues"
  },
  "homepage": "https://swiftcarrot.dev/react-input-color",
