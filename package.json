{
  "name": "react-input-color",
  "version": "2.0.29",
  "description": "React input color component with hsv color picker",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production rollup -c",
